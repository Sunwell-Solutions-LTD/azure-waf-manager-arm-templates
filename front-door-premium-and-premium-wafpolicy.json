            "defaultValue": "Main-Origin-Group",
            "type": "String",
            "metadata": {
                "description": "The name of the Origin Group"
            }
        },
        "WAF_Policy_Name": {
            "defaultValue": "XXXWafPolicy",
            "type": "String",
            "metadata": {
                "description": "The name of the WAF Policy. Cannot have spaces or special characters"
            }
