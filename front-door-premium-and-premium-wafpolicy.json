                                    "matchVariable": "RemoteAddr",
                                    "operator": "IPMatch",
                                    "negateCondition": true,
                                    "matchValue": [
                                        "185.138.63.1/32"
                                    ],
                                    "transforms": []
                                }
                            ],
                            "action": "Block"
                        },
